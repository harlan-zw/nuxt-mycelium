{
  "name": "nuxt-mycelium-monorepo",
  "version": "0.0.7",
  "private": true,
  "packageManager": "pnpm@7.6.0",
  "scripts": {
    "changelog-md": "auto-changelog --commit-limit false --hide-credit",
    "build": "pnpm -r --parallel --filter=./modules/** run prepack",
    "dev": "pnpm -r --parallel --filter=./modules/** run dev:prepare",
    "play": "cd playground && pnpm dev",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "release": "bumpp package.json theme/package.json --commit --push --tag",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "bumpp": "^8.2.1",
    "changelogen": "^0.1.1",
    "eslint": "^8.21.0",
    "unbuild": "^0.7.6",
    "vitest": "^0.21.0"
  }
}
